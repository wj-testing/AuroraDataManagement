<%@ page language="java" contentType="text/html; charset=gb2312"
    pageEncoding="gb2312"%>

<form name="importForm" method="POST" action="import">
	<div class="contents">
		<div class="grid_wrapper">
			<div class="g_6 contents_header">
				<h3 class="i_16_forms tab_label">导入</h3>
			</div>
			
			<div class="g_12 separator"><span></span></div>
			
			<div class="g_12">
				<div class="widget_header">
					<h4 class="widget_header_title wwIcon i_16_forms">导入</h4>
				</div>
				<div class="widget_contents noPadding">
					<div class="line_grid">
						<div class="g_3"><span class="label">请选择要导入的文件</span></div>
						<div class="g_9">
							<input type="file" class="simple_form" />
						</div>
					</div>
					<div class="line_grid">
						<div class="g_3"><span class="label">确认</span></div>
						<div class="g_9">
							<input type="submit" value="上传" class="simple_buttons" />
						</div>
					</div>
				</div>
		      </div>
	     </div>
	</div>
</form>